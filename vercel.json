{
    "env": {
        "FLASK_ENV": "production"
    },
    "routes": [
        {
            "src": "/static/(.*)",
            "dest": "../static/$1"
        },
        {
            "src": "/templates/(.*)",
            "dest": "../templates/$1"
        },
        {
            "src": "/(.*)",
            "dest": "/api/$1"
        }
    ]
  }